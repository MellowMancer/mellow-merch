{
  "name": "mellow-merch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server/server.ts",
    "client:dev": "npm run dev --prefix client",
    "client:build": "npm run build --prefix client",
    "client:lint": "npm run lint --prefix client",
    "dev:all": "concurrently \"npm run dev\" \"npm run client:dev\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.21.2",
    "dotenv": "^16.4.5",
    "http-errors": "~1.6.3",
    "jade": "^1.9.2",
    "morgan": "^1.10.1",
    "mongoose": "^8.8.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "@types/uuid": "^9.0.8",
    "vite": "npm:rolldown-vite@7.1.14",
    "concurrently": "^9.1.2"
  }
}
